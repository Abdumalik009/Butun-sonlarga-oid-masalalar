import java.util.Scanner;

public class Integer24 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);


        System.out.print("Sonni kiriting: ");
        int K = scanner.nextInt();


        int y = (K + 1) % 7;


        String kunNom = " ";
        switch (y) {
            case 0:
                kunNom = "Yakshanba";
                break;
            case 1:
                kunNom = "Dushanba";
                break;
            case 2:
                kunNom = "Seshanba";
                break;
            case 3:
                kunNom = "Chorshanba";
                break;
            case 4:
                kunNom = "Payshanba";
                break;
            case 5:
                kunNom = "Juma";
                break;
            case 6:
                kunNom = "Shanba";
                break;
            default:
                System.out.println("Noto'g'ri sanani kiritdingiz.");
                return;
        }


        System.out.println("Son " + kunNom + " hafta kuniga to'g'ri keladi");
    }
}
