import java.util.Scanner;

public class Integer21 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Kun boshidan o'tgan vaqtni kriting sekunda:  ");
        int son = scanner.nextInt();

        int m = son/60;


        System.out.println("Kun boshidan shuncha " + m +" minut o'tgan");
        System.out.println("Kun boshidan shuncha " + son +" sekund o'tgan");

    }
}
