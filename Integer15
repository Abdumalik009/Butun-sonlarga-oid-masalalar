import java.util.Scanner;

public class Integer15 {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        System.out.print("3 xonali sonni kiriting: ");
        int son = scanner.nextInt();


        int yuzlar = son / 100;


        int onlar = (son /  10)%10;

        int  birlar = son %  10;



        System.out.println("O'nliklar va birliklarni almashgan holdagi hosil bo'lgan misol: " + onlar + yuzlar + birlar);




    }
}
