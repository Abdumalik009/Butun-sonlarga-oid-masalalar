import java.util.Scanner;

public class Integer6 {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        System.out.print("2 xonali sonni kiriting: ");
        int son = scanner.nextInt();

        int onliklar = son / 10;
        int birlar = son % 10;
        System.out.println("10 liklar xonasidagi raqam: " + onliklar);
        System.out.println("Birlar xonasidagi raqam: " + birlar);


    }
}
